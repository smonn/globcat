{
  "name": "globcat",
  "version": "0.5.0",
  "description": "Concatenate files from command line with glob pattern.",
  "keywords": [
    "glob",
    "wildcard",
    "file pattern",
    "concatenate",
    "command line"
  ],
  "main": "globcat.js",
  "scripts": {
    "lint": "eslint globcat.js test/*Spec.js func/*.js",
    "precodestyle": "npm run lint",
    "codestyle": "jscs globcat.js test/*Spec.js func/*.js",
    "pretest": "npm run codestyle",
    "test": "mocha --harmony",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "watch": "onchange globcat.js test/*Spec.js func/*.js -- npm test"
  },
  "bin": {
    "globcat": "bin/globcat.js"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "combined-stream2": "^1.1.2",
    "command-line-args": "^2.1.6",
    "glob": "^7.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "is-stream": "^1.1.0",
    "jscs": "^3.0.3",
    "mocha": "^2.4.5",
    "onchange": "^2.4.0"
  },
  "bugs": {
    "url": "https://github.com/smonn/globcat/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/smonn/globcat.git"
  },
  "files": [
    "bin/globcat.js",
    "func",
    "globcat.js"
  ],
  "homepage": "https://github.com/smonn/globcat#readme",
  "author": "Simon Ingeson <simon.ingeson@gmail.com> (http://www.ingeson.se/)",
  "license": "MIT"
}
